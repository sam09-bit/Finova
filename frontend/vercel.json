{
  "rewrites": [
    {
      "source": "/api/yahoo/:path*",
      "destination": "[https://query1.finance.yahoo.com/:path](https://query1.finance.yahoo.com/:path)*"
    },
    {
      "source": "/ai-endpoint/:path*",
      "destination": "[https://prishaa-stockpredictor.hf.space/api/analyze](https://prishaa-stockpredictor.hf.space/api/analyze)"
    },
    {
      "source": "/api/twelve/:path*",
      "destination": "[https://api.twelvedata.com/:path](https://api.twelvedata.com/:path)*"
    },
    {
      "source": "/api/alpha/:path*",
      "destination": "[https://www.alphavantage.co/:path](https://www.alphavantage.co/:path)*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
