{
  "rewrites": [
    {
      "source": "/api/yahoo/(.*)",
      "destination": "https://query1.finance.yahoo.com/$1"
    },
    {
      "source": "/ai-endpoint",
      "destination": "https://prishaa-stockpredictor.hf.space/api/analyze"
    },
    {
      "source": "/api/twelve/(.*)",
      "destination": "https://api.twelvedata.com/$1"
    },
    {
      "source": "/api/alpha/(.*)",
      "destination": "https://www.alphavantage.co/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}